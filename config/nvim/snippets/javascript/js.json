{
  "JSDoc Comment": {
    "prefix": "/*",
    "body": [
      "/**",
      " * ${1:Comment}$0",
      "*/"
    ],
    "description": "A JSDoc comment"
  },
  "console.log": {
    "prefix": "clg",
    "body": "console.log(${0})"
  },
  "return": {
    "prefix": "ret",
    "body": "return ${0}"
  },
  "if-else statement": {
    "prefix": "if",
    "body": [
      "if (${1:condition}) {",
      "\t$0",
      "} else {",
      "\t",
      "}"
    ],
    "description": "If-Else Statement"
  },
  "switch statement": {
    "prefix": "switch",
    "body": [
      "switch (${1:key}) {",
      "\tcase ${2:value}:",
      "\t\t$0",
      "\t\tbreak;",
      "",
      "\tdefault:",
      "\t\tbreak;",
      "}"
    ],
    "description": "Switch Statement"
  },
  "map": {
    "prefix": ".map",
    "body": ".map((${2:item}) => {\n\t${0}\n})"
  },
  "reduce": {
    "prefix": ".reduce",
    "body": ".reduce((${2:previous}, ${3:current}) => {\n\t${0}\n}${4:, initial})"
  },
  "filter": {
    "prefix": ".filter",
    "body": ".filter((${2:item}) => {\n\t${0}\n})"
  },
  "find": {
    "prefix": ".find",
    "body": ".find((${2:item}) => {\n\t${0}\n})"
  },
  "every": {
    "prefix": ".every",
    "body": ".every((${2:item}) => {\n\t${0}\n})"
  },
  "some": {
    "prefix": ".some",
    "body": ".some((${2:item}) => {\n\t${0}\n})"
  },
  "import module": {
    "prefix": "imp",
    "body": "import ${2:*} from '${1:module}'"
  },
  "module default export": {
    "prefix": "ed",
    "body": "export default ${1:member}"
  },
  "function": {
    "prefix": "f",
    "body": "const ${1:first} = (${2:second}) => { ${3:third} }"
  },
  "export function": {
    "prefix": "ef",
    "body": "export const ${1:first} = (${2:second}) => { ${3:third} }"
  },
  "module default export function": {
    "prefix": "edf",
    "body": [
      "const ${1:first} = (${2:second}) => { ${3:third} }",
      "",
      "export default ${1:first}"
    ]
  }
}
